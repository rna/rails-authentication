<h1>Sign In</h1>

<%= form_for(:session, url: login_path) do |f| %>

  <%= f.label :email %>
  <%= f.email_field :email %>
  
  <%= f.label :password %>
  <%= f.password_field :password %>
  <br>
  <br>
  <%= f.submit "Login" %> 
  <%= link_to '<button type="button">Register</button>'.html_safe, signup_path, method: "get" %> 
  
<% end %>